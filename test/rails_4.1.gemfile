# Gemfile for health_test testing

source 'https://rubygems.org'

ruby '1.9.3' if RUBY_VERSION < '1.9.3'

gem 'rails', "~> 4.1.0"
gem 'rake', '>= 0.8.3'
gem "rack", '~> 1.5.2'

group :development, :test do
  if defined?(JRUBY_VERSION)
    gem 'jruby-openssl'
    gem 'activerecord-jdbcsqlite3-adapter'
  else
    gem 'sqlite3', "~> 1.3.7"
  end
  gem 'shoulda'
end
